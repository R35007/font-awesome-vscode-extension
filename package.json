{
	"name": "font-awesome",
	"displayName": "Font Awesome",
	"description": "Copy Font awesome Icons as HTML, React, Vue, SVG, Base64, Unicode code snippet. Save icon as png or svg.",
	"version": "1.0.0",
	"icon": "images/logo.png",
	"publisher": "Thinker",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/R35007/font-awesome-vscode-extension"
	},
	"author": {
		"name": "Sivaraman",
		"email": "sivaraman.muralidharan@verzion.com"
	},
	"keywords": [
		"font",
		"font awesome",
		"font awesome icons",
		"icons",
		"icons library",
		"icons collection",
		"icon snippets",
		"free icons",
		"svg icons",
		"base64 icons",
		"unicode icons",
		"html icons",
		"react icons",
		"vue icons",
		"custom icons"
	],
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"activationEvents": [
		"onCommand:font-awesome.search-icons",
		"onView:font-awesome.icons-view"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Font Awesome",
			"properties": {
				"font-awesome.settings.customIconsFolderPath": {
					"type": "string",
					"description": "Provide a Custom Icons Folder Path."
				},
				"font-awesome.settings.customIcons": {
					"type": "object",
					"additionalProperties": true,
					"properties": {
						"Custom Style": {
							"type": "array",
							"additionalItems": true,
							"items": {
								"type": "object",
								"examples": [
									{
										"name": "file-arrow-down",
										"label": "File arrow down",
										"objectID": "i:solid:file-arrow-down",
										"unicode": "f56d",
										"ranking": "",
										"family": "solid",
										"className": "fa-solid fa-file-arrow-down",
										"html": "<i class=\"fa-solid fa-file-arrow-down\"></i>",
										"react": "<FontAwesomeIcon icon=\"fa-solid fa-file-arrow-down\" />",
										"vue": "<font-awesome-icon icon=\"fa-solid fa-file-arrow-down\" />",
										"svg": "<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 384 512\"><!--! Font Awesome Free 6.2.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free (Icons: CC BY 4.0, Fonts: SIL OFL 1.1, Code: MIT License) Copyright 2022 Fonticons, Inc. --><path d=\"M64 0C28.7 0 0 28.7 0 64V448c0 35.3 28.7 64 64 64H320c35.3 0 64-28.7 64-64V160H256c-17.7 0-32-14.3-32-32V0H64zM256 0V128H384L256 0zM216 232V334.1l31-31c9.4-9.4 24.6-9.4 33.9 0s9.4 24.6 0 33.9l-72 72c-9.4 9.4-24.6 9.4-33.9 0l-72-72c-9.4-9.4-9.4-24.6 0-33.9s24.6-9.4 33.9 0l31 31V232c0-13.3 10.7-24 24-24s24 10.7 24 24z\"/></svg>",
										"base64": "data:image/svg+xml;base64,",
										"categories": [
											"files"
										],
										"keywords": [
											"filearrowdown",
											"solid",
											"f56d",
											"fasolidfafilearrowdown",
											"i:solid:filearrowdown",
											"filedownload",
											"files",
											"document",
											"export",
											"save"
										]
									}
								],
								"properties": {
									"name": {
										"type": "string"
									},
									"label": {
										"type": "string"
									},
									"objectID": {
										"type": "string"
									},
									"unicode": {
										"type": "string"
									},
									"ranking": {
										"type": "integer"
									},
									"family": {
										"type": "string"
									},
									"className": {
										"type": "string"
									},
									"html": {
										"type": "string"
									},
									"react": {
										"type": "string"
									},
									"vue": {
										"type": "string"
									},
									"svg": {
										"type": "string"
									},
									"base64": {
										"type": "string"
									},
									"categories": {
										"type": "array",
										"items": {
											"type": "string"
										}
									},
									"keywords": {
										"type": "array",
										"items": {
											"type": "string"
										}
									}
								},
								"required": [
									"name",
									"label",
									"family",
									"svg"
								]
							}
						}
					},
					"description": "Provide a Custom Icons object that contains list of icon details"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "icons-view",
					"title": "Font Awesome",
					"icon": "images/logo.svg"
				}
			]
		},
		"views": {
			"icons-view": [
				{
					"type": "webview",
					"id": "font-awesome.icons-view",
					"name": "Font Awesome"
				}
			]
		},
		"commands": [
			{
				"command": "font-awesome.refreshView",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "font-awesome.download",
				"title": "Download Icon Archive",
				"icon": "$(arrow-down)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "font-awesome.refreshView",
					"group": "navigation",
					"when": "view == font-awesome.icons-view"
				},
				{
					"command": "font-awesome.download",
					"group": "navigation",
					"when": "view == font-awesome.icons-view"
				}
			]
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "xml",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "html",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "scss",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "css",
				"path": "./snippets/fa-icons-snippets.json"
			},
			{
				"language": "less",
				"path": "./snippets/fa-icons-snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "14.x",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"@vscode/webview-ui-toolkit": "^0.8.4",
		"convert-svg-to-png": "^0.6.4",
		"potrace": "^2.1.8"
	}
}